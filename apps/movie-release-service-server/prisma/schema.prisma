datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ReleaseInfo {
  createdAt   DateTime  @default(now())
  id          String    @id @default(cuid())
  info        String?
  releaseDate DateTime?
  updatedAt   DateTime  @updatedAt
}

model MovieRequest {
  country   String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  tmdbId    String?
  updatedAt DateTime @updatedAt
}
